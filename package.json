{
  "name": "tripable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.8",
    "husky": "^8.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --ignore-unknown --write"
    ],
    "*": "prettier --ignore-unknown --write"
  }
}
